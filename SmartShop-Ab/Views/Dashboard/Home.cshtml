
<body style="background-image: url('https://media.istockphoto.com/photos/law-and-justice-concept-picture-id934890574?k=20&m=934890574&s=612x612&w=0&h=eBJXRoaXSECKf69iJBXAmkAxPnx78At8gGU5EaXCKS4='); margin: 0px;
background-repeat: no-repeat; background-size: cover;">
 @{
    try
    {
        if(TempData["msg"]!=null)
        {
            foreach(var x in (List<SmartShop_Ab.Models.AddProduct>)TempData["msg"])
            {
                <div class="border border-info border-0 p-4 text-white">
                               @x.Id<br/>
                               @x.ProductName<br/>
                               @x.BrandType<br/>
                               @x.Quantity<br/>
                               @x.StockCount<br/>
                              
                       </div>
                       <br/>
            }
        }
    }
    catch
    {

    }
 }
 </body>
